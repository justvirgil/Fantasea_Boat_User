<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="16dp"
    app:cardCornerRadius="30dp"
    app:cardElevation="5dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <TextView
            android:id="@+id/TextView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:fontFamily="casual"
            android:text="Agency Username: "
            android:textSize="15sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/agencyUsernameTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@id/TextView11"
            android:ellipsize="end"
            android:fontFamily="casual"
            android:maxLines="1"
            android:text="Agency Username"
            android:textSize="15sp"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/TextView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/TextView11"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:fontFamily="casual"
            android:text="Boat Name: "
            android:textSize="15sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/boatNameTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/TextView11"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@id/TextView3"
            android:ellipsize="end"
            android:fontFamily="casual"
            android:maxLines="1"
            android:text="Boat Name"
            android:textSize="15sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/TextView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/TextView3"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:fontFamily="casual"
            android:text="Seating capacity: "
            android:textSize="15sp"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/boatSeatCapTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/boatNameTextView"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@id/TextView2"
            android:fontFamily="casual"
            android:text="Seating Capacity"
            android:textSize="15sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/TextView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/TextView2"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:fontFamily="casual"
            android:text="Status:"
            android:textSize="15sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/pumpboatStatusTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/boatSeatCapTextView"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@+id/TextView6"
            android:ellipsize="end"
            android:fontFamily="casual"
            android:maxLines="1"
            android:text="Status"
            android:textSize="15sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/TextView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/TextView6"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:fontFamily="casual"
            android:text="Date:"
            android:textSize="15sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/date_queuedTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/pumpboatStatusTextView"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@+id/TextView7"
            android:ellipsize="end"
            android:fontFamily="casual"
            android:maxLines="1"
            android:text="date queued"
            android:textSize="15sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/TextView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/TextView7"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:fontFamily="casual"
            android:text="Time:"
            android:textSize="15sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/time_queuedTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/date_queuedTextView"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@+id/TextView8"
            android:ellipsize="end"
            android:fontFamily="casual"
            android:maxLines="1"
            android:text="time queued"
            android:textSize="15sp"
            android:textStyle="bold" />



    </RelativeLayout>

</androidx.cardview.widget.CardView>